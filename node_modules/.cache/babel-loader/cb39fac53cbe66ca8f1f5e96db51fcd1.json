A,GAAA,IAAA;;AAFA,oBAGA,KAAA,CAAA,IAHA;AAAA;AAAA;AAAA;;AAIA,gBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,MAAA,GAAA,IAAA;AACA,gBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA,GAAA,aAAA;AALA,iDAMA,KANA;;AAAA;AAAA,oBAQA,KAAA,CAAA,IARA;AAAA;AAAA;AAAA;;AASA,gBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,MAAA,GAAA,IAAA;AACA,gBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA,GAAA,cAAA;AAVA,iDAWA,KAXA;;AAAA;AAAA;AAeA,gBAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AAfA;AAAA,uBAgBA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,SAAA,EAAA;AACA,kBAAA,MAAA,EAAA;AACA,oBAAA,GAAA,EAAA,KAAA,CAAA;AADA;AADA,iBAAA,CAhBA;;AAAA;AAgBA,gBAAA,QAhBA;AAqBA,gBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,oBAAA,QAAA,IAAA,QAAA,CAAA,IAAA,EAAA;AACA,kBAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AACA;;AAxBA;AAAA;;AAAA;AAAA;AAAA;AA0BA,gBAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AA1BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4BA,KAlCA;AAoCA,IAAA,WApCA,uBAoCA,IApCA,EAoCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AAFA;AAAA;AAAA,uBAIA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA,WAAA,EAAA;AACA,kBAAA,MAAA,EAAA;AACA,oBAAA,IAAA,EAAA,IAAA,CAAA,IADA;AAEA,oBAAA,EAAA,EAAA,IAAA,CAAA,EAFA;AAGA,oBAAA,KAAA,EAAA,MAAA,CAAA,IAHA;AAIA,oBAAA,MAAA,EAAA,MAAA,CAAA;AAJA;AADA,iBAAA,CAJA;;AAAA;AAIA,gBAAA,QAJA;;AAYA,oBAAA,QAAA,IAAA,QAAA,CAAA,IAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA,CAAA,UAAA,GAAA,EAAA,IAAA,EAAA;AACA,wBAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,sBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA;AACA;;AACA,oBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA;AACA,sBAAA,GAAA,EAAA,IAAA,CAAA,eADA;AAEA,sBAAA,IAAA,EAAA,IAAA,CAAA,IAFA;AAGA,sBAAA,IAAA,EAAA;AAHA,qBAAA;AAKA,2BAAA,GAAA;AACA,mBAVA,EAUA,EAVA,CAAA;AAWA;;AAxBA;AAAA;;AAAA;AAAA;AAAA;AA0BA,gBAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AA1BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4BA;AAhEA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"schedule\">\r\n    <div class=\"container\">\r\n      <div class=\"schedule__box\">\r\n        <div class=\"schedule__top\">\r\n          <div class=\"schedule__left\">\r\n            <v-btn\r\n              @click=\"getStudents()\"\r\n              :loading=\"loading\"\r\n              :disabled=\"loading\"\r\n              class=\"schedule__btn btn-orange btn--medium btn--center\"\r\n            >\r\n              Студент\r\n            </v-btn>\r\n\r\n            <div class=\"schedule__input\">\r\n              <v-input\r\n                v-model=\"name\"\r\n                @input=\"clearError('name')\"\r\n                :error=\"errors.name\"\r\n                class-el=\"inpt__input--default\"\r\n              />\r\n              <span\r\n                @click=\"name = '', clearError('name')\"\r\n                class=\"schedule__clear\"\r\n              >\r\n                <img src=\"@/assets/icons/close.svg\">\r\n              </span>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"schedule__datepicker\">\r\n            <date-picker\r\n              v-model=\"date\"\r\n              @change=\"clearError('date'), (current ? getSchedule(current) : '')\"\r\n              format=\"DD.MM.YYYY\"\r\n              value-type=\"YYYY-MM-DD\"\r\n              placeholder=\"ДД.ММ.ГГГГ\"\r\n            />\r\n            <transition\r\n              name=\"fade\"\r\n              mode=\"out-in\"\r\n            >\r\n              <div\r\n                v-if=\"errors.date.status\"\r\n                class=\"error__text\"\r\n              >\r\n                {{ errors.date.text }}\r\n              </div>\r\n            </transition>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"schedule__list\">\r\n          <div\r\n            @click=\"getSchedule(item)\"\r\n            v-for=\"(item, i) in list\"\r\n            :key=\"i\"\r\n            class=\"schedule__link\"\r\n          >\r\n            <div class=\"schedule__link-name\">\r\n              {{ item.label }}\r\n            </div>\r\n            <div class=\"schedule__link-desc\">\r\n              {{ item.description }}\r\n            </div>\r\n          </div>\r\n\r\n          <div\r\n            v-for=\"(item, i) in schedule\"\r\n            :key=\"i\"\r\n            class=\"schedule__item\"\r\n          >\r\n            <div class=\"schedule__date\">\r\n              {{ item[0].day }}\r\n              <span>\r\n                {{ $dayjs(i, 'YYYY.MM.DD').format('DD') }}\r\n              </span>\r\n              {{ $dayjs(i, 'YYYY.MM.DD').format('MMMM') }}\r\n            </div>\r\n\r\n            <div class=\"schedule__lessons\">\r\n              <div\r\n                v-for=\"(child, c) in item\"\r\n                :key=\"c\"\r\n                class=\"schedule__lesson\"\r\n              >\r\n                <div class=\"schedule__info\">\r\n                  <div class=\"schedule__time\">\r\n                    {{ child.item.beginLesson }} - {{ child.item.endLesson }}\r\n                  </div>\r\n                  {{ child.item.lessonNumberStart }}-я пара\r\n                </div>\r\n\r\n                <div class=\"schedule__block\">\r\n                  <div class=\"schedule__name\">\r\n                    {{ child.item.discipline }}\r\n                  </div>\r\n                  <div class=\"schedule__row\">\r\n                    <div class=\"schedule__icon\">\r\n                      <img src=\"@/assets/icons/type.svg\">\r\n                    </div>\r\n                    <div class=\"schedule__value schedule__value--type\">\r\n                      {{ child.item.kindOfWork }}\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"schedule__row\">\r\n                    <div class=\"schedule__icon\">\r\n                      <img src=\"@/assets/icons/location.svg\">\r\n                    </div>\r\n                    <div class=\"schedule__value\">\r\n                      {{ child.item.auditorium }} <span>({{ child.item.building }})</span>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"schedule__row\">\r\n                    <div class=\"schedule__icon\">\r\n                      <img src=\"@/assets/icons/user.svg\">\r\n                    </div>\r\n                    <div class=\"schedule__value\">\r\n                      {{ child.item.lecturer }}\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'PageSchedule',\r\n  data () {\r\n    return {\r\n      name: '',\r\n      date: '',\r\n      list: null,\r\n      schedule: null,\r\n      current: null,\r\n      loading: false,\r\n      errors: {\r\n        name: {\r\n          status: false,\r\n          text: ''\r\n        },\r\n        date: {\r\n          status: false,\r\n          text: ''\r\n        }\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    clearError (type) {\r\n      this.errors[type].status = false\r\n      this.errors[type].text = ''\r\n    },\r\n\r\n    async getStudents () {\r\n      this.schedule = null\r\n      this.current = null\r\n      if (!this.name) {\r\n        this.errors.name.status = true\r\n        this.errors.name.text = 'Введите ФИО'\r\n        return false\r\n      }\r\n      if (!this.date) {\r\n        this.errors.date.status = true\r\n        this.errors.date.text = 'Укажите дату'\r\n        return false\r\n      }\r\n\r\n      try {\r\n        this.loading = true\r\n        const response = await this.$api.get('/search', {\r\n          params: {\r\n            str: this.name\r\n          }\r\n        })\r\n        this.loading = false\r\n        if (response && response.data) {\r\n          this.list = response.data\r\n        }\r\n      } catch (e) {\r\n        console.log('err: ', e)\r\n      }\r\n    },\r\n\r\n    async getSchedule (item) {\r\n      this.list = null\r\n      this.current = item\r\n      try {\r\n        const response = await this.$api.get('/schedule', {\r\n          params: {\r\n            type: item.type,\r\n            id: item.id,\r\n            start: this.date,\r\n            finish: this.date\r\n          }\r\n        })\r\n        if (response && response.data) {\r\n          this.schedule = response.data.reduce((arr, item) => {\r\n            if (!arr[item.date]) {\r\n              arr[item.date] = []\r\n            }\r\n            arr[item.date].push({\r\n              day: item.dayOfWeekString,\r\n              date: item.date,\r\n              item\r\n            })\r\n            return arr\r\n          }, {})\r\n        }\r\n      } catch (e) {\r\n        console.log('err: ', e)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/Schedule"}]}